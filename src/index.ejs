    <body>
        <div id="overlay">
          <div>
            <button id="startButton">Loading...</button>
            <p>Audio playback requires user interaction.</p>
          </div>
        </div>
        <div id="progress"></div>
        <div id="controls_container">
            <div class=left>
            <div id="info"></div>
            <div id="controls_inner">
                <svg id="back" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="32" height="32" viewBox="0 0 32 32">
                  <image id="normal" width="24" height="32" xlink:href="data:img/png;base64,"/>
                  <image id="hover" width="24" height="32" xlink:href="data:img/png;base64,"/>
                </svg>
                    <svg id="play" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="32" height="32" viewBox="0 0 32 32">
                      <image id="normal" data-name="play" width="32" height="32" xlink:href="data:img/png;base64,"/>
                      <image id="hover" data-name="play" width="32" height="32" xlink:href="data:img/png;base64,"/>
                    </svg>
                    <svg id="pause" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="32" height="32" viewBox="0 0 32 32">
                      <image id="normal" data-name="pause" width="32" height="32" xlink:href="data:img/png;base64,"/>
                      <image id="hover" data-name="pause" width="32" height="32" xlink:href="data:img/png;base64,"/>
                    </svg>
                    <svg id="forward" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="32" height="32" viewBox="0 0 32 32">
                      <image id="normal" x="8" width="24" height="32" xlink:href="data:img/png;base64,"/>
                      <image id="hover" x="8" width="24" height="32" xlink:href="data:img/png;base64,"/>
                    </svg>
            </div>
            </div>
        </div>
